<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.badlogic.ui">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.badlogic.Jetpack.MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.imgFullStatus"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.badlogic.Jetpack.LiveDataActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.imgFullScreen"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.badlogic.Jetpack.ViewModelBindingActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"
            android:screenOrientation="portrait">
        </activity>


        <activity android:name="com.example.roombasic.MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name="com.teaphy.diffutildemo.MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"
            >
        </activity>
        <activity android:name="com.teaphy.diffutildemo.DiffActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"/>
        <activity android:name="com.teaphy.diffutildemo.AsyncListDifferActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"/>
        <activity android:name="com.teaphy.diffutildemo.ListAdapterActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"/>
        <activity android:name="com.teaphy.diffutildemo.SortedListActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat"></activity>


        <!-- android:theme="@style/Theme.AppCompat" -->
        <activity
            android:name="com.example.android.codelabs.navigation.MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme"
            >
        </activity>

        <receiver android:name="com.example.android.codelabs.navigation.DeepLinkAppWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/navigation_deep_link_appwidget_info" />
        </receiver>

    </application>

</manifest>